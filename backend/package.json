{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "build": "rm -rf dist/ && tsc -p tsconfig.json && cp .env ./dist && --watch",
    "start": "npm run build:ts && npm start -l info dist/index.js",
    "dev": " node dist/index.js && watch:ts && build",
    "server": "npm-run-all --parallel watch:ts serve",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "serve": "nodemon dist/index.js"
  },
  "keywords": [
    "shoppersDen",
    "trust"
  ],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/fastify": "^1.0.12",
    "@fastify/autoload": "^5.8.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.31",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "fastify-cors": "^6.1.0",
    "jest": "^29.7.0",
    "kysely": "^0.27.3",
    "mongoose": "^8.4.0",
    "pino-pretty": "^11.1.0"
  },
